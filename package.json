    "eslint-plugin-react": "^7.11.0",
    "expect": "^1.20.2",
    "min-react-env": "^1.0.0",
    "mocha": "^6.0.0",
    "proxyquire": "^2.0.1",
    "react": "^16.5.0",
    "react-docgen": "^4.0.0",
    "react-dom": "^16.5.0",
    "react-test-renderer": "^16.5.0",
    "rollup": "^1.0.0",
