  "description": "YouTube player component for React.",
  "main": "dist/react-youtube.js",
  "module": "dist/react-youtube.es.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "rollup -c",
    "test:lint": "eslint --cache --fix .",
    "test:types": "tsd",
