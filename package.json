  "bugs": {
    "url": "https://github.com/u-wave/react-youtube/issues"
  },
  "homepage": "https://github.com/u-wave/react-youtube#readme",
  "dependencies": {
    "@types/react": "^0.14.0 || ^15.0.0 || ^16.0.0",
    "@types/youtube": "0.0.38",
    "load-script2": "^1.0.1",
    "prop-types": "^15.6.1"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0"
